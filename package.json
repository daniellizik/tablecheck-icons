{
  "name": "tablecheck-icons",
  "version": "0.0.0-development",
  "description": "TableCheck Icons are an icon set and icon font based on 16px size, and compatible with [Font Awesome](https://fontawesome.com) version 5.x.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tablecheck/tablecheck-icons.git"
  },
  "keywords": [
    "font",
    "awesome",
    "fontawesome",
    "icon",
    "svg",
    "bootstrap",
    "tablecheck"
  ],
  "author": "TableCheck Inc.",
  "license": "(CC-BY-4.0 AND MIT)",
  "bugs": {
    "url": "https://github.com/tablecheck/tablecheck-icons/issues"
  },
  "homepage": "https://github.com/tablecheck/tablecheck-icons#readme",
  "main": "index.js",
  "module": "index.es.js",
  "files": [
    "fonts",
    "svg",
    "*.js"
  ],
  "scripts": {
    "build:fonts": "node scripts/svg2Font.js",
    "build:js": "node scripts/svg2Js.js",
    "prepare": "node scripts/prepublish.js",
    "cleanup": "node scripts/postpublish.js",
    "test": "npm run build:js && jest"
  },
  "release": {
    "prepare": [
      "prepare"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@commitlint/travis-cli": "8.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/react-fontawesome": "0.1.4",
    "babel-jest": "24.8.0",
    "cheerio": "1.0.0-rc.3",
    "colors": "1.3.3",
    "fs-extra": "8.1.0",
    "glob": "7.1.4",
    "glyphs2font": "1.2.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "jest-image-snapshot": "2.9.0",
    "js-yaml": "3.13.1",
    "jsdom-screenshot": "3.2.0",
    "lodash": "4.17.15",
    "path": "0.12.7",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  }
}
